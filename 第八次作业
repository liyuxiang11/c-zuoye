#include<iostream>
#include <iostream>
using namespace std;
int main(){
    int n,m;
    cin>>n;
    int a[4];
    m=n;
    if(0<=n&&n<=2359){
        for(int i=0;i<4;i++){
            a[i]=m%10;//1234
            m=m/10;
        }
        int c=a[0]+a[1]*10;//34
        int b=a[2]+a[3]*10;//12
        if(b>=8){
            b-=8;
            b*=100;
            n=b+c;
            cout<<n;
        }
        else{
            b+=16;
            b*=100;
            n=b+c;
            cout<<n;
        }   
    }
    return 0;

int MyStrstr(char str1[], char str2[])
{
	int i, j;

	for ( i = 0; i < strlen(str1); )
	{
		int x = i;

		for (j = 0; j < strlen(str2); )
		{		
			if (str1[x] == str2[j])
			{
				x++;
				j++;
			}
			else
			{
				i++;
				break;
			}

		}

		if (j == strlen(str2))
		{
			return i;
		}
	}

	return -1;
}

int main()
{
	char str1[500];
	char str2[500];

	cin >> str1 >> str2;

	cout << MyStrstr(str1, str2) << endl;

	return 0;
}
